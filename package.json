{
  "name": "internal-tools",
  "version": "1.21.13",
  "private": true,
  "repository": "https://github.com/renovatebot/internal-tools.git",
  "license": "AGPL-3.0",
  "author": "Rhys Arkins <rhys@arkins.net>",
  "contributors": [
    "Michael Kriese <michael.kriese@visualon.de>"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf dist/",
    "compile": "cd src && ncc build ./index.ts -o ../dist -s --target es2021",
    "eslint": "eslint .",
    "eslint:fix": "yarn eslint --fix",
    "lint": "run-s eslint prettier",
    "postinstall": "husky install",
    "prettier": "prettier \"{**/*,*}.*\" --check --ignore-unknown",
    "prettier:fix": "prettier \"{**/*,*}.*\" --write --ignore-unknown",
    "release": "run-s clean compile semantic-release",
    "semantic-release": "semantic-release",
    "start": "run-s compile && node dist",
    "test:node": "NODE_ENV=debug INPUT_COMMAND=docker-builder INPUT_DRY_RUN=true INPUT_CONFIG=e2e/node.json INPUT_IMAGE_PREFIX=containerbase/ INPUT_IMAGE=node run-s start",
    "test:testing": "NODE_ENV=debug INPUT_COMMAND=docker-builder INPUT_DRY_RUN=true INPUT_CONFIG=e2e/testing.json INPUT_IMAGE_PREFIXES=ghcr.io/containerbase/ INPUT_PLATFORMS=linux/amd64,linux/arm64 run-s start"
  },
  "resolutions": {
    "eslint-plugin-import/tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/exec": "1.1.1",
    "@actions/github": "5.1.1",
    "@actions/io": "1.1.3",
    "@sindresorhus/is": "4.6.0",
    "auth-header": "1.0.0",
    "chalk": "4.1.2",
    "core-js": "3.32.0",
    "escape-string-regexp": "4.0.0",
    "find-up": "5.0.0",
    "got": "11.8.6",
    "renovate": "36.42.4",
    "semver": "7.5.4",
    "shelljs": "0.8.5",
    "source-map-support": "0.5.21",
    "strip-ansi": "7.1.0",
    "tslib": "2.6.1"
  },
  "devDependencies": {
    "@jest/globals": "29.6.2",
    "@renovate/eslint-plugin": "https://github.com/renovatebot/eslint-plugin#v0.0.5",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.4",
    "@semantic-release/npm": "10.0.4",
    "@tsconfig/node16": "1.0.4",
    "@tsconfig/strictest": "2.0.1",
    "@types/auth-header": "1.0.2",
    "@types/jest": "29.5.3",
    "@types/node": "16.18.40",
    "@types/semver": "7.5.0",
    "@types/shelljs": "0.8.12",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "@vercel/ncc": "0.36.1",
    "@yarnpkg/sdks": "3.0.0-rc.48",
    "conventional-changelog-conventionalcommits": "6.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.46.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-formatter-gha": "1.4.2",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-typescript-enum": "2.1.0",
    "husky": "8.0.3",
    "jest": "29.6.2",
    "jest-extended": "4.0.1",
    "nock": "13.3.2",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.1",
    "prettier-plugin-packagejson": "2.4.5",
    "pretty-quick": "3.1.3",
    "rimraf": "5.0.1",
    "semantic-release": "21.0.7",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.6.1",
  "engines": {
    "node": "^16.13.0 || >=18.12.0",
    "yarn": "^3.2.0"
  }
}
